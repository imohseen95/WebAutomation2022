<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name ="Facebook Test Automation Script" parallel="false">
    <test name = "Facebook">

        <parameter name="useCloudEnv" value="false"/>
        <parameter name="envName" value="browserstack"/>
        <parameter name="os" value="OS X"/>
        <parameter name="osVersion" value="Sierra"/>
        <parameter name="browserName" value="chrome"/>
        <parameter name="browserVersion" value="80.0"/>
        <parameter name="url" value="https://www.Facebook.com/"/>
        <classes>

            <class name="FB.TestFBLanguage"/>
            <class name="FB.TestSignUP"/>
            <class name="FB.TestMessenger"/>

        </classes>

    </test>

</suite>



